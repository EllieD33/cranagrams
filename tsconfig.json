{
  "compilerOptions": {
    "jsx": "react-jsx",  
    "target": "ESNext", 
    "module": "ESNext",  
    "moduleResolution": "node", 
    "esModuleInterop": true,    
    "strict": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "lib": ["dom", "esnext"] 
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.d.ts"
  ]
}
